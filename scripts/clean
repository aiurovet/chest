#!/bin/sh

set -Ee

################################################################################
# This script removes generated files from xnx/examples directory
################################################################################

set -x

DIR="$(dirname "${0}")"
HAS=""

################################################################################

echo ""

for d in \
    "${DIR}/mkicons/png" \
    "${DIR}/../out"
do
  if [ -d "${d}" ]; then
    HAS="1"
    echo "Cleaning: \"${d}\""
    rm -rf "${d}"
  fi
done

################################################################################

if [ -n "${HAS}" ]; then
  echo ""
fi

echo "The cleanup successfully completed"
echo ""
exit 0

################################################################################
